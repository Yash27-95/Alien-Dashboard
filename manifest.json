{
    "manifest_version": 3,
    "name": "Alien Dashboard",
    "version": "1.0.0",
    "description": "Just for practicing async JS",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_icon": "./icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
